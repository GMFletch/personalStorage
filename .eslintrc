{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": "warn",
    "no-global-assign": "warn",
    "no-redeclare": "warn",
    "no-undef": "warn",
    "no-unused-vars": [
      "off",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ],
    "no-useless-escape": "warn",
    "no-var": "warn",
    "prefer-const": "warn"
  },
  "globals": {
    "components": "readonly",
    "slide": "readonly",
    "utils": "readonly",
    "didUtils": "readonly",
    "autorun": "readonly",
    "controls": "readonly",
    "getFromSlide": "readonly",
    "window": "readonly",
    "document": "readonly",
    "ggbApplet": "readonly"
  }
}
